{
    "name": "noxxienl/nladdresslexer",
    "description": "Package to evaluate and parse dutch addresses",
    "type": "library",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick",
            "email": "info@noxxie.nl"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "doctrine/lexer": "^2.1|^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.0.0",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "noxxienl\\nladdresslexer\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
